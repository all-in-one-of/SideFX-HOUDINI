#
# Houdini Environment Settings
#
# The contents of this file are read into the environment
# at startup.  They will override any existing entries in
# the environment.
#
# The syntax is one entry per line as follows:
#    VAR = VALUE
#
# Values may be quoted
#    VAR = "VALUE"
#
# Values may be empty
#    VAR = 
#

# Example:
#
# HOUDINI_NO_SPLASH = 1

HOUDINI_NO_SPLASH = 1


# QLIB variable should point to the downloaded and extracted qLib package
# ($HIH is the $HOME/houdiniXX.X folder)
#
QLIB=C:/Users/test/Documents/houdini15.5
QOTL=$QLIB/otls
GIT=C:/Users/test/Documents/houdini15.5/Projects/SideFX-HOUDINI

HOUDINI_OTLSCAN_PATH = $HIH/otls;$HIH/otls/wip;$QOTL/base;$QOTL/future;$QOTL/experimental;$GIT/Assets/Operators;$GIT/Assets/Generators;@/otls
HOUDINI_GALLERY_PATH = $QLIB/gallery;@/gallery
HOUDINI_TOOLBAR_PATH = $QLIB/toolbar;@/toolbar
HOUDINI_SCRIPT_PATH = $QLIB/scripts;@/scripts
